*
  border: 0
  margin: 0
  padding: 0
  box-sizing: border-box
  
body
  // font: 24px 'Helvetica Neue'
  font: 24px 'Roboto', 'Helvetica Neue'
  color: white
  absolute: left right bottom top
  overflow: hidden
  
a
  text-decoration: none
  
button
  outline: none
  cursor: pointer
  
._absolute
  absolute: top left right bottom

._fixed
  fixed: top left right bottom
  
._overflow-y
  overflow-y: auto
  overflow-x: hidden
  
._ellipsis
  overflow: ellipsis
  
.loading-beach
  width: 100%
  text-align: center
  absolute: top 48% left 
  
ul li 
  list-style: none